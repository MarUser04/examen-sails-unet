        </thead>
        <tbody>
          <%if (typeof actores !== 'undefined') {%>
            <% for (let i = 0; i < actores.length; i++) { %>
              <tr>
                <td scope="row">
                  <%= JSON.stringify(actores[i]) %>
                </td>
                <td>sp</td>
                <td>sp</td>
                <td scope="row" class="d-flex justify-content-around">
                  <button style="margin:0.25em" class="btn btn-primary">Actualizar</button>
                  <button style="margin:0.25em" class="btn btn-danger">Eliminar</button>
                  <button style="margin:0.25em" class="btn btn-primary">Ver detalles</button>
                </td>
              </tr>
            <% } %>
          <%}%>
          </tr>
        </tbody>
      </table>
    </div>
